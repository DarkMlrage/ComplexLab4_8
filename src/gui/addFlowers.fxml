<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #191970;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.MainMenuController">
   <children>
      <ChoiceBox fx:id="flowerChoiceBox" layoutX="225.0" layoutY="115.0" prefWidth="150.0" />
      <Label alignment="CENTER" layoutX="154.0" layoutY="22.0" prefHeight="78.0" prefWidth="292.0" text="Select Flower to add" textAlignment="CENTER" textFill="WHITE">
         <font>
            <Font size="32.0" />
         </font>
      </Label>
      <Button layoutX="265.0" layoutY="294.0" mnemonicParsing="false" onAction="#backToBouquet" text="Back" textFill="RED">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Button layoutX="263.0" layoutY="231.0" mnemonicParsing="false" onAction="#addFlowers" text="ADD">
         <font>
            <Font size="21.0" />
         </font>
      </Button>
      <TextField fx:id="count" layoutX="200.0" layoutY="183.0" promptText="Input count of flowers">
         <font>
            <Font size="16.0" />
         </font>
      </TextField>
   </children>
</AnchorPane>
