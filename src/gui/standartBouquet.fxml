         <font>
            <Font size="32.0" />
         </font>
      </Label>
      <Button layoutX="265.0" layoutY="165.0" mnemonicParsing="false" onAction="#addStandarBouquet" text="ADD">
         <font>
            <Font size="21.0" />
         </font>
      </Button>
      <Button layoutX="267.0" layoutY="233.0" mnemonicParsing="false" onAction="#backToMainFromBouquet" text="Back" textFill="RED">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
